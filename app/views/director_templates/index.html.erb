<h1>List of all directors</h1>

<table border="1">
<% @list_of_directors.each do |a_director| %>
  <tr>
    <td><%= a_director.id %></td>
    <td>
      <img src="http://robohash.org/<%= a_director.image %>">
    </td>
    <td><%= a_director.name %></td>
    <td><%= a_director.dob %></td>
    <td>
      <a href="/directors/<%= a_director.id %>">
        Show details
      </a>
    </td>
  </tr>
<% end %>
</table>